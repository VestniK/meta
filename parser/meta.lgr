<identifier> -> letter(letter|digit)*
<qname> -> <identifier>('.'<identifier>)+
<num> -> digit+
<eof> -> 0 // end of input

digit -> {'0'..'9'}
letter -> {'a'..'z' | 'A'..'Z' | '_'}

{whitespace} -> (\t | \r | \n | ' ')+
{commentline} -> '/' '/' neol*
{commentblock} -> '/' '*' na* '*'+ (nans na* '*'+)* '/'

any -> { 1..255 }
neol -> { any - \r - \n }
na -> { any - '*' }
nans -> { na - '/' }

\t ->  9 // tab
\n -> 10 // newline
\r -> 13 // return
