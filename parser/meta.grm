{ '+' '-' } <<  // lower priority
{ '*' '/' } <<  // higher priority

Goal -> [Function]... <eof>

Function -> <identifier> '(' [Arg]','... ')' '{' Expr ';' '}'
Arg -> <identifier>

Expr -> Expr '+' Expr
     -> Expr '-' Expr
     -> Expr '*' Expr
     -> Expr '/' Expr
     -> '(' Expr ')'
     -> <num>
     -> Arg
     -> Call

Call -> <identifier> '(' [Expr]/','... ')'
