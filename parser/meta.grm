{ '+' '-' } <<  // lower priority
{ '*' '/' } <<  // higher priority

Goal -> Source <eof>

Source -> Package [Function]...

Package -> 'package' (<qname> | <identifier>) ';' => onPackage

Function -> <identifier> '(' [Arg]/','... ')' '{' 'return' Expr ';' '}' *> Function
Arg -> <identifier>

Expr -> Expr ('+' | '-' | '*' | '/') Expr
     -> '(' Expr ')'
     -> <num>
     -> Arg
     -> Call

Call -> <identifier> '(' [Expr]/','... ')'
