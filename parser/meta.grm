{ '+' '-' } <<  // lower priority
{ '*' '/' } <<  // higher priority

Goal -> Source <eof>

Source -> Package [Function]...

Package -> 'package' (<qname> | <identifier>) ';'

Function -> <identifier> '(' [Arg]/','... ')' '{' 'return' Expr ';' '}'
Arg -> <identifier>

Expr -> Expr '+' Expr
     -> Expr '-' Expr
     -> Expr '*' Expr
     -> Expr '/' Expr
     -> '(' Expr ')'
     -> <num>
     -> Arg
     -> Call

Call -> <identifier> '(' [Expr]/','... ')'
